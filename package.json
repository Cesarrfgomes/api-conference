{
	"name": "api-conference",
	"version": "1.0.0",
	"description": "",
	"main": "dist/app.js",
	"scripts": {
		"dev": "tsx --watch src/app.ts",
		"build": "tsc",
		"build:exe": "npm run build && pkg dist/app.js --targets node18-win-x64 --output api-conference.exe",
		"build:exe:all": "npm run build && pkg dist/app.js --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --output api-conference",
		"install-service": "npm run build && node scripts/install-service.js",
		"uninstall-service": "node scripts/uninstall-service.js",
		"start-service": "net start \"API Conference\"",
		"stop-service": "net stop \"API Conference\"",
		"test-build": "npm run build && node dist/app.js"
	},
	"pkg": {
		"assets": [
			"node_modules/**/*"
		],
		"targets": [
			"node18-win-x64",
			"node18-linux-x64",
			"node18-macos-x64"
		],
		"outputPath": "executables"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"type": "commonjs",
	"dependencies": {
		"@fastify/cors": "^11.0.1",
		"@fastify/jwt": "^9.1.0",
		"@fastify/swagger": "^9.5.1",
		"@fastify/swagger-ui": "^5.2.3",
		"@types/node": "^24.0.7",
		"@types/oracledb": "^6.6.1",
		"dotenv": "^17.0.0",
		"fastify": "^5.4.0",
		"knex": "^3.1.0",
		"lodash": "^4.17.21",
		"oracledb": "^6.8.0",
		"pg": "^8.16.3",
		"swagger": "^0.7.5",
		"tsup": "^8.5.0",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"zod": "^3.25.67"
	},
	"devDependencies": {
		"@types/lodash": "^4.17.20",
		"pkg": "^5.8.1",
		"node-windows": "^1.0.0-beta.8"
	}
}
